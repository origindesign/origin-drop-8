
<div class="clearfix row listing ajax {{ additional_classes }} {% if not filter_ajax %} no-filters {% endif %}">

    {% if listing_heading %}
        {{ listing_heading|raw }}
    {% endif %}

    {% if filter_ajax %}
        <div class="clearfix filters filter-ajax">
            {{ filter_ajax }}
        </div>
    {% endif %}

    <div class="content">
        {{ content }}
    </div>

</div>