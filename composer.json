{
    "name": "origindesign/origin-drupal-8",
    "description": "Install Origin starting Install of Drupal 8 with Composer.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.4",
        "bower-asset/lazysizes": "^5.2",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.0",
        "drupal/address": "^1.3",
        "drupal/admin_toolbar": "^2.0",
        "drupal/advagg": "^4.0",
        "drupal/components": "^2.0",
        "drupal/config_direct_save": "^1.0",
        "drupal/core": "^8.7",
        "drupal/core-composer-scaffold": "^8.8.1",
        "drupal/core-recommended": "^8.8",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^4.0",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/field_group": "^3.0",
        "drupal/hsts": "^1.0",
        "drupal/jquery_ui_datepicker": "^1.0",
        "drupal/lazy": "^3.4",
        "drupal/link_attributes": "^1.5",
        "drupal/linkit": "^6.0",
        "drupal/menu_block": "^1.5",
        "drupal/metatag": "^1.5",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.2",
        "drupal/redirect": "^1.2",
        "drupal/redis": "^1.0",
        "drupal/role_delegation": "^1.0",
        "drupal/simple_block": "^1.0@beta",
        "drupal/sitemap": "^2.0",
        "drupal/smart_trim": "^1.1",
        "drupal/tinypng": "^1.0@alpha",
        "drupal/token": "^1.0",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^2.4",
        "drupal/upgrade_status": "^3.0",
        "drupal/view_unpublished": "^1.0",
        "drupal/views_bulk_edit": "^2.4",
        "drupal/views_bulk_operations": "^3.3",
        "drupal/weight": "^3.1",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush-ops/behat-drush-endpoint": "^9.3",
        "drush/drush": "8.*",
        "kint-php/kint": "^3.3",
        "oomphinc/composer-installers-extender": "~2",
        "origindesign/efq": "^2.0",
        "origindesign/helper": "^2.0",
        "pantheon-systems/drupal-integrations": "^8",
        "pantheon-systems/quicksilver-pushback": "^2",
        "tinify/tinify": "1.4.*"
    },
    "require-dev": {
        "behat/behat": "^3.1",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "dmore/behat-chrome-extension": "^1.3",
        "drupal/coder": "^8.3.1",
        "drupal/drupal-driver": "^2.1",
        "drupal/drupal-extension": "^4.1",
        "genesis/behat-fail-aid": "^2.1",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3.1",
        "mikey179/vfsstream": "^1.2",
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.4.0",
        "symfony/css-selector": "^2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "build-assets": [
            "@prepare-for-pantheon",
            "composer install --optimize-autoloader --no-dev"
        ],
        "lint": "find web/modules/custom web/themes/custom -name '*.php' -exec php -l {} \\;",
        "code-sniff": [
            "./vendor/bin/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor ./web/modules/custom",
            "./vendor/bin/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor ./web/themes/custom",
            "./vendor/bin/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor ./web/modules/custom",
            "./vendor/bin/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor ./web/themes/custom"
        ],
        "prepare-for-pantheon": "DrupalProject\\composer\\ScriptHandler::prepareForPantheon",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-create-project-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "build-env": {
            "install-cms": [
                "drush site-install standard --account-mail={account-mail} --account-name={account-name} --account-pass={account-pass} --site-mail={site-mail} --site-name={site-name} --yes"
            ],
            "export-configuration": "drush config-export --yes"
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[web-root]/sites/development.services.yml": false
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        },
        "process-timeout": 0
    }
}
